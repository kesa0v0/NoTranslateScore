{
  "manifest_version": 3,
  "name": "Chrome 번역 태그 제거기",
  "version": "1.0",
  "description": "Chrome 번역 기능이 추가하는 <font> 태그를 자동으로 제거합니다.",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_end"
    }
  ]
}